<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.sabre.db.PromoCodes" table="PromoCodes">
    <id name="id" column="id" type="int">
      <generator class="native">
      </generator>
    </id>
    <property name="offeringName" type="java.lang.String" update="true" insert="true" column="offeringName" />
    <property name="promoCode" type="java.lang.String" update="true" insert="true" column="promoCode" />
  </class>
</hibernate-mapping>
